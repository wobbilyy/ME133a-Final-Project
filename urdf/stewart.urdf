  </joint>

  <link name="leg5-tip"/>
  
  
  ##### LEG 6
  
  # 1.25sin(60) 1.25cos (60) = y
  # rotate -60
  <joint name="leg6-pitch" type="continuous">
    <parent link="world"/>
    <child  link="leg6-uj-shaft"/>
    <origin xyz="1.0825317 0.625 -0.2" rpy="0 0 -1.047197"/>
