  </link>


  <joint name="leg3-roll" type="continuous">
    <parent link="leg3-uj-shaft"/>
    <child  link="leg3-slider-base"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis   xyz="0 1 0"/>
